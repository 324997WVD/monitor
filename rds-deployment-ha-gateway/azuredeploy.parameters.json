{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "adminPassword": {
      "value": "{enter_existing_adminUsername_password_here}"
    },
    "adminUsername": {
      "value": "cloudadmin"
    },
    "adDomainName": {
      "value": "contoso.com"
    },
    "numberOfWebGwInstances": {
      "value": 1
    },
    "brokerServer": {
      "value": "rdcb-01.contoso.com"
    },
    "WebURL": {
      "value": "{enter_external_fqdn_here(example: gateway.contoso.com)}"
    },
    "domainNetbios": {
      "value": "contoso"
    },
    "dnsLabelPrefix": {
      "value": "{enter_external_fqdn_prefix_here (example:gateway)}"
    },
    "storageAccountName": {
      "value": "{enter_existing_storage_account_here}"
    },
    "gw-availabilityset": {
      "value": "gw-availabilityset"
    },
    "loadBalancer": {
      "value": "loadBalancer"
    },
    "backendAddressPools": {
      "value": "backendAddressPool"
    },
    "existingVnet": {
      "value": "vnet"
    },
    "existingSubnet": {
      "value": "Subnet"
    },
    "_artifactsLocation": {
      "value": "https://raw.githubusercontent.com/Azure/azure-quickstart-templates/master/rds-deployment-ha-gateway/"
    }
  }
}