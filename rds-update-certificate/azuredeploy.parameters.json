    "contentVersion": "1.0.0.0",
    "parameters": {
        "vaultName": {
            "value": "{enter_existing_vault_name_here}"
        },
        "certificateName": {
            "value": "{enter_existing_vault_certificate_name_here}"
        },
        "applicationId": {
            "value": "{enter_existing_vault_certificate_application_id_here}"
